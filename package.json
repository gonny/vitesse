{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "tailwind": "npx tailwind -i ./src/styles/base-layout.tw.css -o ./src/styles/base-layout.css -c ./tailwind.config.cjs --watch",
    "tailwind:build": "tailwindcss -i ./src/styles/base-layout.tw.css -o ./src/styles/base-layout.css -c ./tailwind.config.cjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@unhead/vue": "^1.8.2",
    "@vueuse/core": "^10.5.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vite-ssg": "^0.23.4",
    "vue": "^3.3.7",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^9.6.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.0.18",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@pnpm/eslint-config": "^1.1.0",
    "@stylistic/eslint-plugin": "^1.0.0",
    "@types/markdown-it": "^13.0.5",
    "@types/markdown-it-link-attributes": "^3.0.3",
    "@types/node": "^18.18.8",
    "@types/nprogress": "^0.2.2",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue-macros/volar": "^0.17.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "cypress": "^13.4.0",
    "cypress-vite": "^1.4.2",
    "eslint": "^8.52.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.18.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shikiji": "^0.6.12",
    "pnpm": "^8.10.2",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prettier-plugin-vue": "^1.1.6",
    "sharp": "^0.32.6",
    "shikiji": "^0.6.12",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.5",
    "taze": "^0.12.0",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.6.2",
    "unplugin-vue-markdown": "^0.24.3",
    "vite": "^4.5.0",
    "vite-imagetools": "^6.2.1",
    "vite-plugin-inspect": "^0.7.41",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-webfont-dl": "^3.8.0",
    "vite-ssg-sitemap": "^0.6.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue,ts,tsx,mjs,mts,json}": [
      "eslint --fix"
    ],
    "locales/*.{json,yaml,yml}": [
      "eslint --fix"
    ]
  }
}